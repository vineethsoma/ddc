{
  "name": "ddc",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "start": "./node_modules/.bin/ng serve",
    "build": "./node_modules/.bin/ng build",
    "test": "./node_modules/.bin/ng test --code-coverage",
    "test:single-run": "./node_modules/.bin/ng test --single-run",
    "serve:coverage": "./node_modules/.bin/http-server -c-1 -o -p 9875 ./coverage",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "./node_modules/.bin/ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update:check",
    "update:skip": "./node_modules/.bin/nx update:skip",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "postinstall": "./node_modules/.bin/nx postinstall",
    "help": "./node_modules/.bin/nx help",
    "customer-portal": "ng serve -a customer-portal -p 4200",
    "clear-web-container": "docker stop web && docker rm web",
    "clear-web-image": "docker rmi ddc_web",
    "clear-docker": "npm run clear-web-container && npm run clear-web-image",
    "build-prod": "ng build --prod --app=customer-portal && gzip -r dist/*.js",
    "build-prod-no-gzip": "ng build --prod --app=customer-portal",
    "serve-prod": "npm run build-prod && docker-compose up",
    "serve-dist-http": "./node_modules/.bin/http-server -c-1 -o -p 8080 ./dist",
    "serve-prod-http": "npm run build-prod-no-gzip && npm run serve-dist-http"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.7",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "5.2.7",
    "@angular/compiler": "5.2.7",
    "@angular/core": "5.2.7",
    "@angular/forms": "5.2.7",
    "@angular/material": "^5.2.5",
    "@angular/platform-browser": "5.2.7",
    "@angular/platform-browser-dynamic": "5.2.7",
    "@angular/router": "5.2.7",
    "@ngrx/effects": "5.2.0",
    "@ngrx/entity": "5.2.0",
    "@ngrx/router-store": "5.0.1",
    "@ngrx/store": "5.2.0",
    "@ngrx/store-devtools": "5.2.0",
    "@nrwl/nx": "1.0.2",
    "bootstrap": "^4.1.0",
    "core-js": "^2.4.1",
    "fullcalendar": "^3.9.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "ngrx-store-freeze": "^0.2.1",
    "popper.js": "^1.14.3",
    "primeng": "^5.2.5",
    "rxjs": "^5.5.6",
    "viz": "^0.0.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "5.2.7",
    "@angular/language-service": "5.2.7",
    "@ngrx/schematics": "5.2.0",
    "@nrwl/schematics": "1.0.2",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.10.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.2"
  }
}
